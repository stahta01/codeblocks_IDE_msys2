<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Help wx3.1.x (MSys2)" />
		<Option pch_mode="0" />
		<Option default_target="help_plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="help_plugin">
				<Option output="../../../devel31_msys2/share/CodeBlocks/plugins/help_plugin.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs31_msys2/plugins/contrib/help_plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel31_msys2/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DNOPCH" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="bzip2" />
					<Add directory="zlib" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="bz2" />
					<Add library="z" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9    ../../../devel31_msys2/share/codeblocks/help_plugin.zip manifest.xml *.xrc" />
					<Add after="zip -Rq9 ../../../devel31_msys2/share/CodeBlocks/help_plugin.zip images/16x16/*.png images/20x20/*.png images/24x24/*.png" />
					<Add after="zip -Rq9 ../../../devel31_msys2/share/CodeBlocks/help_plugin.zip images/28x28/*.png images/32x32/*.png images/40x40/*.png" />
					<Add after="zip -Rq9 ../../../devel31_msys2/share/CodeBlocks/help_plugin.zip images/48x48/*.png images/56x56/*.png images/64x64/*.png" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_TOOLKIT" value="msw" />
				<Variable name="WX_VERSION" value="3.1" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="help_plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="../../../include/scripting/bindings" />
			<Add directory="../../../include/scripting/include" />
			<Add directory="../../../include/scripting/sqplus" />
			<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/$(WX_TOOLKIT)-unicode-$(WX_VERSION)" />
			<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--add-stdcall-alias" />
			<Add option="-Wl,--enable-auto-import" />
			<Add library="codeblocks" />
			<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_xrc-$(WX_VERSION).dll" />
			<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_html-$(WX_VERSION).dll" />
			<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_core-$(WX_VERSION).dll" />
			<Add library="wx_base$(WX_SUFFIX)-$(WX_VERSION).dll" />
			<Add directory="../../../devel31_msys2" />
		</Linker>
		<Unit filename="HelpConfigDialog.cpp" />
		<Unit filename="HelpConfigDialog.h" />
		<Unit filename="MANFrame.cpp" />
		<Unit filename="MANFrame.h" />
		<Unit filename="defs.h" />
		<Unit filename="help_common.cpp" />
		<Unit filename="help_common.h" />
		<Unit filename="help_plugin.cpp" />
		<Unit filename="help_plugin.h" />
		<Unit filename="help_plugin.xrc" />
		<Unit filename="man2html.cpp" />
		<Unit filename="man2html.h" />
		<Unit filename="manifest.xml" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
