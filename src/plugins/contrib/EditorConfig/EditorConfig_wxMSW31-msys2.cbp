<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="EditorConfig wx3.1.x (MSys2)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel31_msys2/share/codeblocks/plugins/EditorConfig" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../../devel31_msys2" />
				<Option object_output="../../../.objs31_msys2/plugins/contrib/EditorConfig" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --verbose -p debug" />
				<Option host_application="./codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DNOPCH" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="../../../include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="../../../sdk/wxscintilla/include" />
					<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/$(WX_TOOLKIT)-unicode-$(WX_VERSION)" />
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_core-$(WX_VERSION).dll" />
					<Add library="wx_base$(WX_SUFFIX)-$(WX_VERSION).dll" />
					<Add directory="../../../devel31_msys2" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel31_msys2\share\CodeBlocks\EditorConfig.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_TOOLKIT" value="msw" />
				<Variable name="WX_VERSION" value="3.1" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=gnu++11" />
		</Compiler>
		<Unit filename="EditorConfig.cpp" />
		<Unit filename="EditorConfig.h" />
		<Unit filename="EditorConfigCommon.h" />
		<Unit filename="EditorConfigUI.cpp" />
		<Unit filename="EditorConfigUI.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/EditorConfigUI.wxs" />
		<Extensions>
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/EditorConfigUI.wxs" src="EditorConfigUI.cpp" hdr="EditorConfigUI.h" fwddecl="1" i18n="1" name="EditorConfigUI" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
