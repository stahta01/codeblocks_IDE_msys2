<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CppCheck/Vera++ wx3.1.x (MSys2)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="usePCH">
				<Option output="../../../devel31_msys2/share/codeblocks/plugins/CppCheck.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs31_msys2/plugins/contrib/CppCheck" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel31_msys2/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="2" />
				<Option projectIncludeDirsRelation="2" />
				<Compiler>
					<Add option="-Winvalid-pch" />
					<Add option="-DCB_PRECOMP" />
					<Add directory="../../../.objs31_msys2/include" />
				</Compiler>
			</Target>
			<Target title="noPCH">
				<Option output="../../../devel31_msys2/share/codeblocks/plugins/CppCheck.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs31_msys2/plugins/contrib/CppCheck" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel31_msys2/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="2" />
				<Option projectIncludeDirsRelation="2" />
				<Compiler>
					<Add option="-DNOPCH" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_TOOLKIT" value="msw" />
				<Variable name="WX_VERSION" value="3.1" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="usePCH;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-std=gnu++11" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/$(WX_TOOLKIT)-unicode-$(WX_VERSION)" />
			<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="codeblocks" />
			<Add library="wx_$(WX_TOOLKIT)$(WX_SUFFIX)_core-$(WX_VERSION).dll" />
			<Add library="wx_base$(WX_SUFFIX)-$(WX_VERSION).dll" />
			<Add directory="../../../devel31_msys2" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel31_msys2/share/codeblocks/CppCheck.zip resources/manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="ConfigPanel.cpp" />
		<Unit filename="ConfigPanel.h" />
		<Unit filename="CppCheck.cpp" />
		<Unit filename="CppCheck.h" />
		<Unit filename="CppCheckListLog.cpp" />
		<Unit filename="CppCheckListLog.h" />
		<Unit filename="wxsmith/ConfigPanel.wxs" />
		<Extensions>
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/ConfigPanel.wxs" src="ConfigPanel.cpp" hdr="ConfigPanel.h" fwddecl="1" i18n="1" name="ConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
